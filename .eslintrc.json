{
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules":  {
        "prettier/prettier": [
            "error"
        ],
        "no-unused-vars": "warn",
        "react/jsx-filename-extension": [1, {
            "extensions": [".js", ".jsx"]}
        ]}
}
